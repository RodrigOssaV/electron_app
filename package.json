{
  "name": "electron_app",
  "productName": "EasyTask",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "electron src/index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/task-list.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/task-list.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodrigOssaV/electron_app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RodrigOssaV/electron_app/issues"
  },
  "homepage": "https://github.com/RodrigOssaV/electron_app#readme",
  "dependencies": {
    "electron": "^18.1.0",
    "electron-reload": "^2.0.0-alpha.1",
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "electron-packager": "^15.5.1"
  }
}
